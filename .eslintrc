{
    "extends": "rallycoding",
    "rules": {
      "no-undef": 0,
      "max-len": 0,
      "global-require": 0,
      "no-underscore-dangle": 0,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "no-nested-ternary": 0
    },
    "globals": {
      "document": false
    }
  }
  